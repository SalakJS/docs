{
  "title": "Salak文档",
  "description": "Salak是基于Koa开发的web应用框架，用于快速开发应用",
  "plugins": [
    "-lunr", 
    "-search", 
    "search-plus",
    "-highlight", 
    "prism", 
    "prism-themes",
    "github"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/SalakJS/salak"
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-duotone-light.css"
      ]
    },
    "fontsettings": {
        "theme": "sepia",
        "family": "sans",
        "size": 2
    }
  }
}
